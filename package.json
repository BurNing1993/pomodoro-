{
  "name": "pomodoro",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main/index.cjs",
  "scripts": {
    "pack:win": "npm run build && electron-builder build --config electron-builder.config.js --win",
    "pack:mac": "npm run build && electron-builder build --config electron-builder.config.js --mac",
    "dev": "node scripts/dev.js",
    "build": "npm run check && node scripts/build.js",
    "check": "npm run check:main && npm run check:renderer",
    "check:main": "tsc --noEmit -p src/main/tsconfig.json",
    "check:renderer": "tsc --noEmit -p src/renderer/tsconfig.json",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BurNing1993/pomodoro.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BurNing1993/pomodoro/issues"
  },
  "homepage": "https://github.com/BurNing1993/pomodoro#readme",
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.1.3",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "typescript": "^4.5.4",
    "vite": "^2.7.3"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
