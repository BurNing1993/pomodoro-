{
  "name": "pomodoro",
  "version": "0.1.8",
  "description": "pomodoro",
  "main": "./dist/main.js",
  "scripts": {
    "pack:win": "npm run build && electron-builder build --config electron-builder.config.js --win",
    "pack:mac": "npm run build && electron-builder build --config electron-builder.config.js --mac",
    "package": "npm run build && electron-builder build --config electron-builder.config.js --publish always",
    "start": "node scripts/dev.js",
    "build": "npm run build:main && npm run build:renderer",
    "build:renderer": "cd src/renderer && npm run build",
    "build:main": "webpack -c config/webpack.main.config.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joey2217/pomodoro.git"
  },
  "keywords": [],
  "author": "Joey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joey2217/pomodoro/issues"
  },
  "homepage": "https://github.com/joey2217/pomodoro#readme",
  "dependencies": {
    "electron-updater": "^4.6.5"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^10.2.4",
    "electron": "^17.1.0",
    "electron-builder": "^22.14.13",
    "prettier": "^2.5.1",
    "ts-loader": "^9.2.7",
    "typescript": "^4.6.2",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  }
}